@using SportskiCentar_ASA.Web.Areas.Administrator.ViewModels;
@model AjaxAdministratoriVM


<table class="table">
    <thead>
        <tr>
            <td>Ime</td>
            <td>Prezime</td>
            <td>Korisničko ime</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.admini)
        {
            <tr>
                <td>@x.ime</td>
                <td>@x.prezime</td>
                <td>@x.username</td>
            </tr>
        }
    </tbody>
</table>